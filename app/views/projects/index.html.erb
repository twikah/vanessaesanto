<% provide(:title, "Projects") %>

<section>
  <h2>Projects</h2>
</section>

<div class="row">
  <% @projects.each do |project| %>
    <div class="column">
      <section class="project">
        <img src="project-1-website">
        <h4><%= link_to project[:name], project_path(project[:id]) %></h4>
      </section>
    </div>
  <% end %>
</div>